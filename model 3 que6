#include<stdio.h>
int main()
{
	int i,arr[5]={10,20,30,40,50};
	int n=sizeof (arr)/sizeof (arr[0]);
	int max=arr[0],min=arr[0];
	for(i=1;i<5;i++)
	{
		if (arr[i]>max)
		{
			max=arr[i];
		}
		if(arr[i]<min)
		{
			min=arr[i];
		}
	}
	printf("maximum number in the array:%d\n",max);
	printf("minimum number in the array:%d\n",min);
	return 0;
}
