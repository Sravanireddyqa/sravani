#include<stdio.h>
int main()
{
	int principal,rate,time,interest,age;
	printf("enter the principal");
	scanf("%d",&principal);
	printf("enter the time");
	scanf("%d",&time);
	if(age>=60)
	{
		rate=12.0;
	}
	else
	{
		rate=10.0;
	}
	interest=principal*rate*time/100;
	printf("the simple interest is %d",interest);
	return 0;
}
